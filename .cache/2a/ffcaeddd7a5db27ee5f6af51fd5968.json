r\n  border-radius: 2rem;\r\n  margin: 0 auto;\r\n}\r\n\r\n.searchbar__form {\r\n  width: 80rem;\r\n  position: relative;\r\n}\r\n\r\n.searchbar__input {\r\n  width: 100%;\r\n  height: 6rem;\r\n  padding: 2rem 6rem;\r\n  border-radius: 100rem;\r\n  border: none;\r\n  font-size: 2rem;\r\n  transition: all 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__input:hover,\r\n.searchbar__input:focus {\r\n  box-shadow: 0 4px 4px rgba(0, 0, 0, 0.12);\r\n  transform: scale(1.1);\r\n}\r\n\r\n.searchbar__input:hover + .searchbar__icon,\r\n.searchbar__input:focus + .searchbar__icon,\r\n.searchbar__input:hover ~ .searchbar__btn,\r\n.searchbar__input:focus ~ .searchbar__btn {\r\n  right: -3rem;\r\n}\r\n\r\n.searchbar__icon {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  display: flex;\r\n  align-items: center;\r\n  color: var(--color-black--light);\r\n  height: 6rem;\r\n  width: 8rem;\r\n  font-size: 2rem;\r\n  padding-left: 2rem;\r\n  cursor: text;\r\n  transition: right 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__btn {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  display: none;\r\n  align-items: center;\r\n  color: var(--color-black--light);\r\n  height: 6rem;\r\n  width: 8rem;\r\n  font-size: 2rem;\r\n  padding-left: 2rem;\r\n  border: none;\r\n  background-color: transparent;\r\n  cursor: pointer;\r\n  transition: all 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__btn:hover {\r\n  color: var(--color-gray);\r\n}\r\n\r\n.searchbar__filters {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.searchbar__btn--sort {\r\n  background-color: transparent;\r\n  border: none;\r\n  font-size: 1.6rem;\r\n  margin-right: 0.5rem;\r\n  transition: color 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__btn--sort:hover {\r\n  color: var(--color-gray);\r\n}\r\n\r\n.searchbar__btn--sort--active {\r\n  font-weight: 700;\r\n}\r\n\r\n.searchbar__btn--sort--active:hover {\r\n  color: var(--color-black);\r\n}\r\n\r\n.searchbar__content {\r\n  display: none;\r\n  border-top: 0.1rem solid var(--color-white);\r\n  background-color: #fff;\r\n  width: 80rem;\r\n  height: 26rem;\r\n  padding: 2rem 6rem;\r\n  margin-top: 1.3rem;\r\n  overflow-y: auto;\r\n  box-shadow: 0 4px 4px rgba(0, 0, 0, 0.12);\r\n  transform: scale(1.1);\r\n}\r\n\r\n.searchbar__list {\r\n  list-style: none;\r\n}\r\n\r\n.searchbar__item {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.searchbar__link {\r\n  display: block;\r\n  width: 100%;\r\n  color: var(--color-highlight--dark);\r\n  text-decoration: none;\r\n  transition: color 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__link:hover {\r\n  color: var(--color-highlight);\r\n}\r\n\r\n.searchbar__details {\r\n  font-size: 1.4rem;\r\n}\r\n\r\n.searchbar__btn--pagination {\r\n  margin: 3rem 0;\r\n  text-align: center;\r\n  display: block;\r\n  width: 100%;\r\n  background-color: transparent;\r\n  border: none;\r\n  font-size: 1.6rem;\r\n  transition: color 0.15s ease-in-out;\r\n}\r\n\r\n.searchbar__btn--pagination:hover {\r\n  color: var(--color-gray);\r\n}\r\n"},"lineCount":208}},"error":null,"hash":"6f29c32e9472480c204fe3f3577450e7","cacheData":{"env":{}}}